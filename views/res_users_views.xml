<odoo>
    <record id="res_users_form" model="ir.ui.view">
        <field name="name">res.users.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">

            <xpath expr="//page[@name='access_rights']" position="after">

                <page name="properties" string="Properties">
                    <field name="property_ids"/>
                </page>


            </xpath>

            <!--            Below Code Insert field:test after field:tz which is inside page:preference-->
            <xpath expr="//page[@name='preferences']//field[@name='tz']" position="after">

                <field name="test"/>


            </xpath>
        </field>
    </record>

</odoo>

