<odoo>
    <record id="estate_property_type_action" model="ir.actions.act_window">
        <field name="name">Type</field>
        <field name="res_model">estate.property.type</field>
        <field name="view_mode">tree,form</field>
    </record>


    <record id="estate_property_type_tree" model="ir.ui.view">
        <field name="name">estate.property.type.tree</field>
        <field name="model">estate.property.type</field>
        <field name="arch" type="xml">
            <tree string="Channel">
                <header>
                    <!--                    <button type="action" name="%(estate_property_action)d" string="Property View"/>-->
                    <button type="object" name="url_test" string="Google"/>
                </header>
                <field name="name"/>
                <field name="offer_count" string="Offer Count"/>
                <field name="sequence" widget="handle"/>
            </tree>
        </field>
    </record>


    <record id="estate_property_type_form" model="ir.ui.view">
        <field name="name">estate.property.type.form</field>
        <field name="model">estate.property.type</field>
        <field name="arch" type="xml">
            <form string="TEST">
                <header>
                    <!--                    <button type="action" name="%(estate_property_action)d" string="Property View"/>-->
                    <button type="object" name="url_test" string="Google"/>
                </header>
                <button class="oe_stat_button" type="action" icon="fa-ticket"
                        name="%(estate_property_offer_action)d">
                    <field name="offer_count" string="Offers" widget="statinfo"/>
                </button>

                <sheet>
                    <!--                    <button class="oe_stat_button" type="action" icon="fa-ticket"-->
                    <!--                            name="%(estate_property_offer_action)d">-->
                    <!--                        <field name="offer_count" string="Offers" widget="statinfo"/>-->
                    <!--                    </button>-->


                    <notebook>
                        <page string='Properties'>
                            <field name="property_ids">
                                <tree>
                                    <field name="name" string="Title"/>
                                    <field name="expected_price"/>
                                    <field name="state" string="Status"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>


            </form>
        </field>
    </record>


    <!--    <record id="estate_property_type_search" model="ir.ui.view">-->
